	<!--Theo địa bàn-->
	<record id="theo_dia_ban_report_tree_view" model="ir.ui.view">
        <field name="name">theo.dia.ban.report.tree</field>
        <field name="model">theo.dia.ban.report</field>
        <field eval="2" name="priority"/>
        <field name="arch" type="xml">
            <tree string="Theo địa bàn">
            	<field name="khu_vuc_id" invisible="1"/>
                <field name="tinh_tp_id" />
                <field name="tong_so_du_an" sum="Tổng số dự án"/>
                <field name="pt_du_an" sum="Tổng phần trăm dự án"/>
                <field name="tong_so_von_dau_tu" sum="Tổng VĐT"/>
                <field name="pt_von_dau_tu" sum="Tổng phần trăm VĐT"/>
            </tree>
        </field>
    </record>
	
	<record id="theo_dia_ban_report_search_view" model="ir.ui.view">
        <field name="name">theo.dia.ban.report.search</field>
        <field name="model">theo.dia.ban.report</field>
        <field name="arch" type="xml">
            <search string="Theo địa bàn">
                <group expand="1" string="Group By...">
                    <filter string="Khu vực" icon="terp-personal" name="khuvuc" context="{'group_by':'khu_vuc_id'}"/>
                </group>
            </search>
        </field>
    </record>
    
    <record id="action_theo_dia_ban_report" model="ir.actions.act_window">
        <field name="name">Theo địa bàn</field>
        <field name="res_model">theo.dia.ban.report</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="theo_dia_ban_report_tree_view"/>
        <field name="context">{'search_default_khuvuc':1,'group_by':[]}</field>
        <field name="search_view_id" ref="theo_dia_ban_report_search_view"/>
    </record>

    <menuitem action="action_theo_dia_ban_report" id="menu_action_theo_dia_ban_report" parent="ql_ho_so_bao_cao" sequence="10"/>
	
