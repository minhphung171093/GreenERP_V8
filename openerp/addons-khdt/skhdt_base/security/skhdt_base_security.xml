<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
		<record model="ir.module.category" id="module_category_skhdt_quantri">
	        <field name="name">SKHDT Quản trị</field>
	        <field name="sequence">2</field>
	    </record>
	
	    <record id="group_skhdt_quantri" model="res.groups">
	        <field name="name">Quản trị</field>
	        <field name="skhdt">1</field>
	        <field name="category_id" ref="module_category_skhdt_quantri"/>
	        <field name="implied_ids" eval="[(4, ref('base.group_hr_manager'))]"/>
	    </record>
		
		<record model="ir.ui.menu" id="base.menu_reporting">
		    <field name="groups_id" eval="[(6, 0, [ref('base.group_system')])]" />
		</record>
		
		<act_window id="base.change_password_wizard_action"
            name="Change Password"
            src_model="res.users"
            res_model="change.password.wizard"
            view_type="form" view_mode="form"
            key2="client_action_multi" target="new"
            groups="group_skhdt_quantri"/>
            
		<record id="base.group_user" model="res.groups">
	    	<field name="skhdt">1</field>
	    </record>
	    
		<record id="base.group_hr_user" model="res.groups">
			<field name="skhdt">1</field>
	    </record>
	    
	    <record id="base.group_hr_manager" model="res.groups">
	    	<field name="skhdt">1</field>
	    </record>
	    
	    <record id="base.group_multi_company" model="res.groups">
	    	<field name="skhdt">1</field>
	    </record>
	    
	    <record id="skhdt_quantri_rule" model="ir.rule">
	        <field name="name">Quản trị</field>
	        <field name="model_id" ref="base.model_res_users"/>
	        <field name="domain_force">[('id','!=',1)]</field>
	        <field name="groups" eval="[(4, ref('group_skhdt_quantri'))]"/>
	    </record>
	    
	</data>
</openerp>
