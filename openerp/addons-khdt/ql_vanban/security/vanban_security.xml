<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
		<record model="ir.module.category" id="module_category_vanban">
	        <field name="name">Quản lý văn bản</field>
	        <field name="sequence">0</field>
	    </record>
	
	    <record id="group_vanban_nv" model="res.groups">
	        <field name="name">Nhập liệu văn bản</field>
	        <field name="skhdt">1</field>
	        <field name="category_id" ref="module_category_vanban"/>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
	    </record>
	    
	    <record id="group_vanban_cauhinh" model="res.groups">
	        <field name="name">Cấu hình quản lý văn bản</field>
	        <field name="skhdt">1</field>
	        <field name="category_id" ref="module_category_vanban"/>
	        <field name="implied_ids" eval="[(4, ref('group_vanban_nv'))]"/>
	    </record>
	
		<record id="group_vanban_tp" model="res.groups">
	        <field name="name">Trưởng phòng quản lý văn bản</field>
	        <field name="skhdt">1</field>
	        <field name="category_id" ref="module_category_vanban"/>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('group_vanban_cauhinh'))]"/>
	    </record>
	
		<record id="skhdt_base.group_skhdt_quantri" model="res.groups">
	        <field name="implied_ids" eval="[(4, ref('group_vanban_tp'))]"/>
	    </record>
	
	</data>
</openerp>
