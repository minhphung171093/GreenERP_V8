<?xml version="1.0" encoding="utf-8"?>
<openerp>
 <data>

 		<record id="quan_huyen_tree_view" model="ir.ui.view">
            <field name="name">quan.huyen.tree</field>
            <field name="model">quan.huyen</field>
            <field eval="2" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Quận/Huyện" editable='top'>
                	<field name="name"/>
                </tree>
            </field>
        </record>
	
   		<record id="quan_huyen_view_form" model="ir.ui.view">
            <field name="name">quan.huyen.form</field>
            <field name="model">quan.huyen</field>
            <field name="arch" type="xml">
                <form string="Quận/Huyện" version="7.0">
                	<sheet>
                		<group>
                			<field name="name"/>
	                    </group>
                    </sheet>
                </form>
            </field>
        </record>
		
		<record id="action_quan_huyen" model="ir.actions.act_window">
            <field name="name">Quận/Huyện</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">quan.huyen</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="quan_huyen_view_form"/>
        </record>
		
   </data>  
</openerp>
