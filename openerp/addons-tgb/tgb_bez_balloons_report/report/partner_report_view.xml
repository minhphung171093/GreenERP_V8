	        <field name="report_rml">tgb_bez_balloons_report/report/quotation_sample.ods</field>
	        <field name="parser_state">loc</field>
	        <field name="tml_source">file</field>
	        <field name="attachment_use" eval="0"/>
    	</record>		    
		<report 
			auto="False" 
			menu="True"
			id="quotation_sample"
			model="sale.order"
			name="quotation_sample"
	        string="Quotation Sample"
